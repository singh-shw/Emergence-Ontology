# Which emergent behaviors have more than [X] features associated with them?

SELECT ?emergentBehavior (COUNT(DISTINCT ?emergenceFeature) as ?featureCount)
WHERE {
    ?emergentBehavior rdf:type BASE:EmergentBehavior .
    ?emergenceFeature rdfs:subPropertyOf BASE:hasEmergentFeature .
    ?emergentBehavior ?emergenceFeature ?value .
    FILTER(?value = true) .
}
GROUP BY ?emergentBehavior
HAVING (?featureCount > X)
ORDER BY DESC(?featureCount)
