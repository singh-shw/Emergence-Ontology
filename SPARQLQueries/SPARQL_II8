# What classifications are provided by [Author_Name]?

SELECT DISTINCT ?classification
WHERE {
    ?author rdf:type BASE:Author .
    ?author BASE:hasName ?name .
    FILTER regex(?name, "{Author_Name}", "i") .
    ?author BASE:defines ?behavior .
    ?behavior rdf:type ?classification .
    ?classification rdfs:subClassOf BASE:EmergentBehavior .
}
