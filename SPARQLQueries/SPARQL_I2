# Which authors have common emergence features in their definitions?

SELECT DISTINCT ?author1Name ?author2Name ?commonFeature
WHERE {
    ?author1 rdf:type BASE:Author .
    ?author1 BASE:hasName ?author1Name .
    ?author1 BASE:defines ?behavior1 .
    ?behavior1 rdf:type ?emergentBehavior1 .
    ?emergentBehavior1 rdfs:subClassOf BASE:EmergentBehavior .
    ?commonFeature rdfs:subPropertyOf BASE:hasEmergentFeature .
    ?behavior1 ?commonFeature ?value1 .
    FILTER(?value1 = true) .

    ?author2 rdf:type BASE:Author .
    ?author2 BASE:hasName ?author2Name .
    ?author2 BASE:defines ?behavior2 .
    ?behavior2 rdf:type ?emergentBehavior2 .
    ?emergentBehavior2 rdfs:subClassOf BASE:EmergentBehavior .
    ?behavior2 ?commonFeature ?value2 .
    FILTER(?value2 = true) .

    FILTER (?author1 != ?author2) .
}
ORDER BY ?author1Name ?author2Name ?commonFeature
