# Which authors have discussed both [EmergentBehavior1] and [EmergentBehavior2]?

SELECT DISTINCT ?authorName
WHERE {
    ?author rdf:type BASE:Author .
    ?author BASE:hasName ?authorName .

    ?author BASE:defines ?behavior1 .
    ?behavior1 rdf:type ?class1 .
    ?class1 rdfs:subClassOf BASE:EmergentBehavior .
    ?class1 owl:equivalentClass {Class1:subClassOf(EmergentBehavior)}  .

    ?author BASE:defines ?behavior2 .
    ?behavior2 rdf:type ?class2 .
    ?class2 rdfs:subClassOf BASE:EmergentBehavior .
    ?class2 owl:equivalentClass {Class2:subClassOf(EmergentBehavior)} .
}
